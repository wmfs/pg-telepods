{
  "name": "@wmfs/pg-telepods",
  "version": "0.0.0-semantically-released",
  "description": "Takes the contents of one PostgreSQL table, applies a transformation function to each row and ensures a target table is kept in sync",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/pg-telepods#readme",
  "keywords": [
    "tymly",
    "package",
    "etl",
    "pg",
    "postgresql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/pg-telepods.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/pg-telepods/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "camelcase-keys": "5.0.0",
    "csv-string": "3.1.6",
    "debug": "4.1.1",
    "lodash": "4.17.15",
    "make-dir": "3.0.0",
    "pg": "7.17.0",
    "pg-copy-streams": "1.2.0",
    "pg-query-stream": "2.0.0",
    "@wmfs/supercopy": "1.15.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.6.1",
    "conventional-changelog-metahub": "3.0.0",
    "cz-conventional-changelog": "3.0.2",
    "mocha": "6.2.2",
    "nyc": "15.0.0",
    "semantic-release": "15.14.0",
    "standard": "12.0.1",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@wmfs/hl-pg-client": "1.13.0"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
