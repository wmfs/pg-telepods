{
  "name": "@wmfs/pg-telepods",
  "version": "0.0.0-semantically-released",
  "description": "Takes the contents of one PostgreSQL table, applies a transformation function to each row and ensures a target table is kept in sync",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/pg-telepods#readme",
  "keywords": [
    "tymly",
    "package",
    "etl",
    "pg",
    "postgresql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/pg-telepods.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/pg-telepods/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "camelcase-keys": "4.2.0",
    "csv-string": "3.1.3",
    "debug": "3.1.0",
    "lodash": "4.17.10",
    "make-dir": "1.3.0",
    "pg": "7.4.3",
    "pg-copy-streams": "1.2.0",
    "pg-query-stream": "1.1.1",
    "@wmfs/supercopy": "1.0.2"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.4",
    "conventional-changelog-metahub": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "15.7.1",
    "standard": "11.0.1",
    "@semantic-release/changelog": "2.1.1",
    "@semantic-release/git": "6.0.1",
    "@wmfs/hl-pg-client": "1.1.3"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
